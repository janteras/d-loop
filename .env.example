# D-Loop Protocol Environment Configuration
# Copy this file to .env and fill in the values before deployment

# Network RPC URLs
SEPOLINA_RPC_URL=https://rpc.sepolina.dev
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Deployment keys - KEEP THESE SECURE!
# Never commit the actual .env file to git
PRIVATE_KEY=

# Etherscan API Key for contract verification
ETHERSCAN_API_KEY=

# Gas Reporter configuration
REPORT_GAS=false
COINMARKETCAP_API_KEY=

# Deployment configuration
DEPLOYER_ADDRESS=
ADMIN_ADDRESS=

# Test configuration
FORKING_ENABLED=false
FORKING_URL=

# Security tools configuration
ECHIDNA_CORPUS_DIR=echidna-corpus
MANTICORE_RESULTS_DIR=manticore-results

# Code coverage
CODECOV_TOKEN=
